<?xml version="1.0"?>
<launch>
    <!-- NAVIGATION SYSTEM -->
    <node pkg="serial_node" 
    type="serial_node_1.py" 
    name="serial_node_1" 
    output="screen">
    <param name="port" value="/dev/ttyUSB0"/>
    </node>

    <node pkg="navigation_system" 
    type="navigation_SM" 
    name="Nav" 
    output="screen">
    </node>

    <!-- VISION SYSTEM -->
    <node pkg="vision_system" 
    type="raw_image_publisher.py" 
    name="raw_image" 
    output="screen">
    </node>

    <node pkg="vision_system" 
    type="coords_image_subscriber.py" 
    name="coords_image_subscriber" 
    output="screen">
    </node>

    <node pkg="vision_system" 
    type="get_coords_server.py" 
    name="get_coords_server" 
    output="screen">
    </node>

    <!-- ARM SYSTEM -->

    <!-- STATE MACHINE -->
    <node pkg="state_machine" 
    type="state_machine.py" 
    name="STATE" 
    output="screen">
    </node>


</launch>
